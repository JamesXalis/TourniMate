    "license": "ISC",
    "bugs": {
        "url": "https://github.com/JamesXalis/TourniMate/issues"
    },
    "homepage": "https://github.com/JamesXalis/TourniMate#readme",
    "devDependencies": {
        "concurrently": "^5.1.0",
        "express": "4.17.1",
        "graphql": "^16.6.0",
        "mongoose": "6.0.13"
    },
    "dependencies": {
        "@stripe/react-stripe-js": "^1.15.0",
        "@stripe/stripe-js": "^1.44.1"
    }
}
